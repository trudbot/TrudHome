{
    "name": "trudhome",
    "manifest_version": 3,
    "version": "1.0.0",
    "author": {
        "name": "trudbot",
        "email": "trudbot@outlook.com"
    },
    "description": "极简中的极简的浏览器起始页。",
    "minimum_chrome_version": "88",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [ "storage" ],
    "background": {
        "service_worker": "./background/index.js",
        "type": "module"
    },
    "homepage_url": "https://github.com/trudbot/TrudHome",
    "host_permissions": [
        "https://suggestion.baidu.com/"
    ],
    "icons": {
        "16": "assets/icons/logo16.png",
        "32": "assets/icons/logo32.png",
        "48": "assets/icons/logo48.png",
        "128": "assets/icons/logo128.png"
    }
}