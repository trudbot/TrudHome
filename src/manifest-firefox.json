{
    "name": "trudhome",
    "manifest_version": 3,
    "version": "1.0.0",
    "author": "trudbot",
    "developer": {
        "name": "trudbot",
        "url": "https://trudbot.cn"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "home@trudbot.cn",
            "strict_min_version": "109.0"
        }
    },
    "description": "极简中的极简的浏览器起始页。",
    "minimum_chrome_version": "88",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [ "storage" ],
    "background": {
        "scripts": ["./background/index.js"]
    },
    "homepage_url": "https://github.com/trudbot/TrudHome",
    "host_permissions": [
        "https://suggestion.baidu.com/"
    ],
    "icons": {
        "16": "assets/icons/logo16.png",
        "32": "assets/icons/logo32.png",
        "48": "assets/icons/logo48.png",
        "128": "assets/icons/logo128.png"
    }
}